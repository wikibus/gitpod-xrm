PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
prefix wbo: <https://schema.wikibus.org/>

<shape/storage-container>
    sh:targetClass wbo:StorageContainer ;
    sh:property
        [
            sh:path schema:containsPlace ;
            sh:class wbo:StorageLocation ;
        ],
        [
            sh:path rdfs:label ;
            sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
        ] ;
.

<shape/storage-location>
    sh:targetClass wbo:StorageLocation ;
    sh:property
        [
            sh:path [ sh:inversePath schema:containsPlace ] ;
            sh:class wbo:StorageContainer ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
        ],
        [
            sh:path rdfs:label ;
            sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:pattern "^.+ \\(\\d+\\)$" ;
        ] ;
.

